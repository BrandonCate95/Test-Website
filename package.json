  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules ./src/index.scss -o ./src",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules --watch ./src/App.scss ./src/App.css ./src/index.scss ./src/index.css",
    "postinstall": "npm run transpile",
    "start:lib": "better-npm-run lib-watch",
    "start:server": "better-npm-run start:server",
    "start": "npm-run-all -p start:lib start:server",
    "build:server": "babel server-env.js --out-file server.js",
    "build:client": "webpack -p --config webpack.prod.js --progress",
    "build": "better-npm-run build",
    "transpile": "better-npm-run transpile"
  },
  "betterScripts": {
    "transpile": {
      "command": "babel node_modules/@material --presets env,react -d node_modules/@material && babel node_modules/apollo-link --presets env,react -d node_modules/apollo-link && babel node_modules/react-apollo --ignore node_modules/react-apollo/node_modules --presets env,react -d node_modules/react-apollo"
    },
    "start:server": {
      "command": "nodemon server-env.js --ignore lib --ignore dist --watch server-env.js --watch api.js --exec babel-node --presets env,react",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "lib": {
      "command": "babel ./src -d ./lib",
      "env": {
        "BABEL_ENV": "lib"
      }
    },
    "lib-watch": {
      "command": "babel ./src --watch -d ./lib",
      "env": {
        "BABEL_ENV": "lib_dev"
      }
    },
    "build": {
      "command": "better-npm-run lib && better-npm-run build:server && npm run build:client",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:server": {
      "command": "babel server-env.js --out-file server.js",
      "env": {
        "BABEL_ENV": "lib"
      }
    }
  }
}
